<div class="profile-page">

  <%= render partial: "users/show", :locals => { :user => @user } %>

  <h4><%= @user.username %>'s Relevant Projects</h4>
  <ul>
    <% @user.projects.each do |project| %>
      <li><%= link_to project.title, project_path(project.id), class: "show-project"  %></li><br />
    <% end %>
  </ul>

</div>

