<div class="profile-page">

  <%= render partial: "users/show", :locals => { :user => @user } %>

  <h4><%= @user.username %>'s Relevant Projects</h4>
  <ul>
    <% @projects.each do |project| %> 
      <% project.skills.where(:skill => @user.skills.ids) %>
    <li>
      <%= project.title %>
    </li>
    <% end %>
  </ul>

</div>

